{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": "2020",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
          "version": "16.12.0"
        }
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-prettier",
        "plugin:react/recommended"
    ],
    "rules": {
        "semi": "warn",
        "no-console":"off",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "no-useless-escape": "off",
        "react/no-unescaped-entities": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
    }
}